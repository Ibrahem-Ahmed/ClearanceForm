{
	"_version": "1.5.0",
	"sap.app": {
		"id": "ClearanceForm",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"FormAlias": {
				"uri": "model/local.json",
				"type": "JSON"
			},
			"ClearanceRequest": {
				"uri": "/destination/alnasser/sap/opu/odata/sap/ZHR_CLEARACE_REQUEST_SRV",
				"type": "OData"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "ClearanceForm.view.SplitApp",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ClearanceForm.i18n.i18n"
				}
			},
			"FormModel": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "FormAlias"
			},
			"ClearanceRequestModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "ClearanceRequest"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewPath": "ClearanceForm.view",
				"viewType": "XML"
			},
			"routes": [
				{
					"pattern": "",
					"name": "SplitApp",
					"view": "SplitApp",
					"controlAggregation": "pages",
					"subroutes": [
						{
							"pattern": "",
							"name": "Master",
							"view": "Master",
							"controlId": "SplitApp",
							"targetAggregation": "masterPages",
							"subroutes": [
								{
									"pattern": "Details/{ClNum}",
									"name": "Details",
									"view": "Details",
									"targetAggregation": "detailPages",
									"viewLevel": 1
								},
								{
									"pattern": "CreateRequest",
									"name": "CreateRequest",
									"view": "CreateRequest",
									"targetAggregation": "detailPages",
									"transition": "flip",
									"viewLevel": 0
								}
							]
						}
					]
				}
			]
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zclearform_tst1/webapp",
		"_version": "1.1.0"
	}
}