<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="ClearanceForm.controller.Master">
	<App>
		<pages>
			<Page title="Master">
				<content>
				    <List noDataText="{i18n>NoData}" id="__list0" items="{/ClearanceFormHeaderSet}" growing="true" growingScrollToLoad="true">
				        <items>
				            <StandardListItem type="Navigation" title="{From}- {To}" description="{FormTypeDesc}" info="{State}" id="__item0" press="GoToLineItem"/>
				            </items>
				    </List>
				</content>
			<footer>
			    <Bar class="sapContrast sapContrastPlus" id="__bar0">
			        <contentRight>
			            <Button text="Create" width="100px" id="__button0" icon="sap-icon://create" press="CreateRequest"/>
			        </contentRight>
			    </Bar>
			</footer>
			</Page>
		</pages>
	</App>
</mvc:View>